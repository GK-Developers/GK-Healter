<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkListStore" id="file_list_store">
    <columns>
      <!-- 0: Toggle -->
      <column type="gboolean"/>
      <!-- 1: Category -->
      <column type="gchararray"/>
      <!-- 2: Description -->
      <column type="gchararray"/>
      <!-- 3: Size String -->
      <column type="gchararray"/>
      <!-- 4: Size Bytes (Hidden) -->
      <column type="gint64"/>
      <!-- 5: Path (Hidden) -->
      <column type="gchararray"/>
      <!-- 6: Is System (Hidden) -->
      <column type="gboolean"/>
    </columns>
  </object>
  <object class="GtkListStore" id="history_list_store">
    <columns>
      <!-- 0: Date -->
      <column type="gchararray"/>
      <!-- 1: Categories -->
      <column type="gchararray"/>
      <!-- 2: Total Freed -->
      <column type="gchararray"/>
      <!-- 3: Status -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkWindow" id="main_window">
    <property name="can_focus">False</property>
    <property name="title" translatable="yes">Sistem Temizleyici</property>
    <property name="default_width">900</property>
    <property name="default_height">650</property>
    <child type="titlebar">
      <object class="GtkHeaderBar">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="title" translatable="yes">Temizleyici</property>
        <property name="show_close_button">True</property>
        <child>
          <object class="GtkButton" id="btn_about">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="tooltip_text" translatable="yes">Hakkında</property>
            <signal name="clicked" handler="on_about_clicked" swapped="no"/>
            <child>
              <object class="GtkImage">
                <property name="visible">True</property>
                <property name="icon_name">view-more-symbolic</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="pack_type">start</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btn_settings">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="tooltip_text" translatable="yes">Ayarlar</property>
            <signal name="clicked" handler="on_settings_clicked" swapped="no"/>
            <child>
              <object class="GtkImage">
                <property name="visible">True</property>
                <property name="icon_name">preferences-system-symbolic</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="pack_type">end</property>
          </packing>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkStack" id="main_stack">
        <property name="visible">True</property>
        <property name="transition_type">slide-left-right</property>
        <property name="transition_duration">400</property>
        
        <!-- Page 1: Main View (Clean/History) -->
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="orientation">vertical</property>
            <property name="spacing">0</property>
            
            <child>
              <object class="GtkInfoBar" id="info_bar">
                <property name="visible">True</property>
                <property name="message_type">info</property>
                <property name="revealed">True</property>
                <child internal-child="content_area">
                  <object class="GtkBox">
                    <property name="spacing">16</property>
                    <property name="margin">10</property>
                    <child>
                      <object class="GtkLabel" id="info_label">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">Analiz için hazır.</property>
                        <property name="wrap">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
              </packing>
            </child>

            <child>
              <object class="GtkNotebook" id="notebook">
                <property name="visible">True</property>
                <property name="margin">10</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="orientation">vertical</property>
                    <property name="spacing">10</property>
                    <property name="margin">10</property>
                    <child>
                      <object class="GtkScrolledWindow">
                        <property name="visible">True</property>
                        <property name="shadow_type">in</property>
                        <property name="min_content_height">400</property>
                        <child>
                          <object class="GtkTreeView" id="treeview">
                            <property name="visible">True</property>
                            <property name="model">file_list_store</property>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Seç</property>
                                <child>
                                  <object class="GtkCellRendererToggle" id="renderer_toggle">
                                    <signal name="toggled" handler="on_cell_toggled" swapped="no"/>
                                  </object>
                                  <attributes>
                                    <attribute name="active">0</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Kategori</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">1</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Açıklama</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">2</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Boyut</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">3</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                      <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkLabel" id="summary_label">
                        <property name="visible">True</property>
                        <property name="label">Kazanılacak Alan: 0 MB</property>
                        <property name="halign">end</property>
                        <property name="margin_top">5</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="visible">True</property>
                        <property name="spacing">10</property>
                        <property name="halign">end</property>
                        <property name="margin_top">10</property>
                        <child>
                          <object class="GtkButton" id="btn_scan">
                            <property name="label">Analiz Et</property>
                            <property name="visible">True</property>
                            <signal name="clicked" handler="on_scan_clicked" swapped="no"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="btn_clean">
                            <property name="label">Temizle</property>
                            <property name="visible">True</property>
                            <property name="sensitive">False</property>
                            <signal name="clicked" handler="on_clean_clicked" swapped="no"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="tab">
                  <object class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="label">Temizlik</property>
                  </object>
                </child>

                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="orientation">vertical</property>
                    <property name="spacing">10</property>
                    <property name="margin">10</property>
                    <child>
                      <object class="GtkScrolledWindow">
                        <property name="visible">True</property>
                        <property name="shadow_type">in</property>
                        <child>
                          <object class="GtkTreeView" id="history_treeview">
                            <property name="visible">True</property>
                            <property name="model">history_list_store</property>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Tarih</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">0</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Kategoriler</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">1</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Kazanç</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">2</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn">
                                <property name="title">Durum</property>
                                <child>
                                  <object class="GtkCellRendererText"/>
                                  <attributes>
                                    <attribute name="text">3</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                      <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="position">1</property>
                  </packing>
                </child>
                <child type="tab">
                  <object class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="label">Geçmiş</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="name">page_main</property>
          </packing>
        </child>

        <!-- Page 2: Settings View (Integrated Full-Window) -->
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="orientation">vertical</property>
            <property name="valign">start</property>
            
            <!-- Settings Header (Simpler/Native) -->
            <child>
              <object class="GtkBox">
                <property name="visible">True</property>
                <property name="margin">12</property>
                <property name="spacing">12</property>
                <child>
                  <object class="GtkButton" id="btn_back">
                    <property name="visible">True</property>
                    <property name="valign">center</property>
                    <signal name="clicked" handler="on_back_clicked" swapped="no"/>
                    <child>
                      <object class="GtkImage">
                        <property name="visible">True</property>
                        <property name="icon_name">go-previous-symbolic</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="lbl_settings_title">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Ayarlar</property>
                    <attributes>
                      <attribute name="weight" value="bold"/>
                      <attribute name="scale" value="1.2"/>
                    </attributes>
                  </object>
                </child>
              </object>
            </child>

            <child>
              <object class="GtkSeparator">
                <property name="visible">True</property>
              </object>
            </child>

            <child>
              <object class="GtkScrolledWindow">
                <property name="visible">True</property>
                <property name="hscrollbar_policy">never</property>
                <property name="vscrollbar_policy">automatic</property>
                <property name="propagate_natural_height">True</property>
                <child>
                  <object class="GtkViewport">
                    <property name="visible">True</property>
                    <property name="shadow_type">none</property>
                    <child>
                      <object class="GtkBox">
                        <property name="visible">True</property>
                        <property name="orientation">vertical</property>
                        <property name="margin">18</property>
                        <property name="spacing">24</property>
                        <property name="valign">start</property>

                        <!-- Group: Dil Ayarları -->
                        <child>
                          <object class="GtkBox">
                            <property name="visible">True</property>
                            <property name="orientation">vertical</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkLabel" id="lbl_settings_language">
                                <property name="visible">True</property>
                                <property name="label" translatable="yes">Dil</property>
                                <property name="xalign">0</property>
                                <attributes>
                                  <attribute name="weight" value="bold"/>
                                </attributes>
                              </object>
                            </child>
                            <child>
                              <object class="GtkListBox">
                                <property name="visible">True</property>
                                <property name="selection_mode">none</property>
                                <property name="margin_left">6</property>
                                <child>
                                  <object class="GtkListBoxRow">
                                    <property name="visible">True</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="visible">True</property>
                                        <property name="margin">10</property>
                                        <property name="spacing">12</property>
                                        <child>
                                          <object class="GtkLabel" id="lbl_language_select">
                                            <property name="visible">True</property>
                                            <property name="label" translatable="yes">Görüntüleme Dili</property>
                                            <property name="xalign">0</property>
                                          </object>
                                          <packing>
                                            <property name="expand">True</property>
                                          </packing>
                                        </child>
                                        <child>
                                          <object class="GtkComboBoxText" id="combo_language">
                                            <property name="visible">True</property>
                                            <signal name="changed" handler="on_language_changed" swapped="no"/>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>

                        <!-- Master Row: Otomatik Bakım -->
                        <child>
                          <object class="GtkBox">
                            <property name="visible">True</property>
                            <property name="orientation">vertical</property>
                            <property name="spacing">16</property>
                            <child>
                              <object class="GtkBox">
                                <property name="visible">True</property>
                                <property name="spacing">12</property>
                                <child>
                                  <object class="GtkBox">
                                    <property name="visible">True</property>
                                    <property name="orientation">vertical</property>
                                    <child>
                                      <object class="GtkLabel" id="lbl_auto_maintenance_title">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Otomatik Temizlik</property>
                                        <property name="xalign">0</property>
                                        <attributes>
                                          <attribute name="weight" value="bold"/>
                                          <attribute name="scale" value="1.1"/>
                                        </attributes>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="lbl_auto_maintenance_desc">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Sistemi belirli aralıklarla temizler.</property>
                                        <property name="xalign">0</property>
                                        <attributes>
                                          <attribute name="scale" value="0.85"/>
                                        </attributes>
                                      </object>
                                    </child>
                                  </object>
                                  <packing>
                                    <property name="expand">True</property>
                                  </packing>
                                </child>
                                <child>
                                  <object class="GtkSwitch" id="switch_auto_maintenance">
                                    <property name="visible">True</property>
                                    <property name="valign">center</property>
                                    <signal name="notify::active" handler="on_auto_maintenance_toggled" swapped="no"/>
                                  </object>
                                </child>
                              </object>
                            </child>

                            <!-- Sub-settings box (Visible only when Master Switch is ON) -->
                            <child>
                              <object class="GtkBox" id="box_auto_settings">
                                <property name="visible">False</property>
                                <property name="orientation">vertical</property>
                                <property name="spacing">18</property>
                                <property name="margin_left">12</property>

                                <child>
                                  <object class="GtkBox">
                                    <property name="visible">True</property>
                                    <property name="orientation">vertical</property>
                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkLabel" id="lbl_scheduling_title">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Zamanlama</property>
                                        <property name="xalign">0</property>
                                        <attributes>
                                          <attribute name="weight" value="bold"/>
                                        </attributes>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkListBox">
                                        <property name="visible">True</property>
                                        <property name="selection_mode">none</property>
                                        <child>
                                          <object class="GtkListBoxRow">
                                            <property name="visible">True</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="visible">True</property>
                                                <property name="margin">10</property>
                                                <child>
                                                  <object class="GtkLabel" id="lbl_frequency_title">
                                                    <property name="visible">True</property>
                                                    <property name="label" translatable="yes">Sıklık</property>
                                                    <property name="xalign">0</property>
                                                  </object>
                                                  <packing>
                                                    <property name="expand">True</property>
                                                  </packing>
                                                </child>
                                                <child>
                                                  <object class="GtkComboBoxText" id="combo_frequency">
                                                    <property name="visible">True</property>
                                                    <signal name="changed" handler="on_frequency_changed" swapped="no"/>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>

                                <child>
                                  <object class="GtkBox">
                                    <property name="visible">True</property>
                                    <property name="orientation">vertical</property>
                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkLabel" id="lbl_conditions_title">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Koşullar</property>
                                        <property name="xalign">0</property>
                                        <attributes>
                                          <attribute name="weight" value="bold"/>
                                        </attributes>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkListBox">
                                        <property name="visible">True</property>
                                        <property name="selection_mode">none</property>
                                        <child>
                                          <object class="GtkListBoxRow">
                                            <property name="visible">True</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="visible">True</property>
                                                <property name="margin">10</property>
                                                <child>
                                                  <object class="GtkLabel" id="lbl_idle_title">
                                                    <property name="visible">True</property>
                                                    <property name="label" translatable="yes">Boşta İken Başlat</property>
                                                  </object>
                                                  <packing>
                                                    <property name="expand">True</property>
                                                  </packing>
                                                </child>
                                                <child>
                                                  <object class="GtkBox">
                                                    <property name="visible">True</property>
                                                    <property name="spacing">6</property>
                                                    <child>
                                                      <object class="GtkSpinButton" id="spin_idle">
                                                        <property name="visible">True</property>
                                                        <property name="adjustment">adj_idle</property>
                                                        <signal name="value-changed" handler="on_idle_changed" swapped="no"/>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="lbl_minutes_suffix">
                                                        <property name="visible">True</property>
                                                        <property name="label" translatable="yes">dk</property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkListBoxRow">
                                            <property name="visible">True</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="visible">True</property>
                                                <property name="margin">10</property>
                                                <child>
                                                  <object class="GtkLabel" id="lbl_ac_power_title">
                                                    <property name="visible">True</property>
                                                    <property name="label" translatable="yes">Sadece Şarjda Çalıştır</property>
                                                  </object>
                                                  <packing>
                                                    <property name="expand">True</property>
                                                  </packing>
                                                </child>
                                                <child>
                                                  <object class="GtkSwitch" id="switch_ac_power">
                                                    <property name="visible">True</property>
                                                    <signal name="notify::active" handler="on_ac_power_toggled" swapped="no"/>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>

                                <child>
                                  <object class="GtkBox">
                                    <property name="visible">True</property>
                                    <property name="orientation">vertical</property>
                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkLabel" id="lbl_notifications_title">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Bildirimler</property>
                                        <property name="xalign">0</property>
                                        <attributes>
                                          <attribute name="weight" value="bold"/>
                                        </attributes>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkListBox">
                                        <property name="visible">True</property>
                                        <property name="selection_mode">none</property>
                                        <child>
                                          <object class="GtkListBoxRow">
                                            <property name="visible">True</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="visible">True</property>
                                                <property name="margin">10</property>
                                                <child>
                                                  <object class="GtkLabel" id="lbl_notify_done_title">
                                                    <property name="visible">True</property>
                                                    <property name="label" translatable="yes">Bittiğinde Haber Ver</property>
                                                  </object>
                                                  <packing>
                                                    <property name="expand">True</property>
                                                  </packing>
                                                </child>
                                                <child>
                                                  <object class="GtkSwitch" id="switch_notify">
                                                    <property name="visible">True</property>
                                                    <signal name="notify::active" handler="on_notify_toggled" swapped="no"/>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>

                                <child>
                                  <object class="GtkExpander" id="expander_advanced">
                                    <property name="visible">True</property>
                                    <property name="can_focus">True</property>
                                    <property name="label" translatable="yes">Gelişmiş Seçenekler</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="visible">True</property>
                                        <property name="orientation">vertical</property>
                                        <property name="margin_top">6</property>
                                        <child>
                                          <object class="GtkListBox">
                                            <property name="visible">True</property>
                                            <property name="selection_mode">none</property>
                                            <child>
                                              <object class="GtkListBoxRow">
                                                <property name="visible">True</property>
                                                <child>
                                                  <object class="GtkBox">
                                                    <property name="visible">True</property>
                                                    <property name="margin">10</property>
                                                    <property name="spacing">12</property>
                                                    <child>
                                                      <object class="GtkLabel" id="lbl_disk_threshold_title">
                                                        <property name="visible">True</property>
                                                        <property name="label" translatable="yes">Disk Doluluğuna Göre Başlat</property>
                                                      </object>
                                                      <packing>
                                                        <property name="expand">True</property>
                                                      </packing>
                                                    </child>
                                                    <child>
                                                      <object class="GtkBox">
                                                        <property name="visible">True</property>
                                                        <property name="spacing">10</property>
                                                        <child>
                                                          <object class="GtkSwitch" id="switch_disk_threshold">
                                                            <property name="visible">True</property>
                                                            <signal name="notify::active" handler="on_disk_threshold_toggled" swapped="no"/>
                                                          </object>
                                                        </child>
                                                        <child>
                                                          <object class="GtkSpinButton" id="spin_disk_percent">
                                                            <property name="visible">True</property>
                                                            <property name="adjustment">adj_disk</property>
                                                            <signal name="value-changed" handler="on_disk_percent_changed" swapped="no"/>
                                                          </object>
                                                        </child>
                                                        <child>
                                                          <object class="GtkLabel">
                                                            <property name="visible">True</property>
                                                            <property name="label" translatable="yes">%</property>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>

                                <child>
                                  <object class="GtkBox">
                                    <property name="visible">True</property>
                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkLabel" id="lbl_last_maintenance_title">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Son Temizlik:</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="lbl_last_maintenance">
                                        <property name="visible">True</property>
                                        <property name="label" translatable="yes">Yapılmadı</property>
                                        <attributes>
                                          <attribute name="style" value="italic"/>
                                        </attributes>
                                      </object>
                                    </child>
                                  </object>
                                </child>

                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="name">page_settings</property>
          </packing>
        </child>
      </object>
    </child>
  </object>

  <object class="GtkAdjustment" id="adj_idle">
    <property name="lower">15</property>
    <property name="upper">60</property>
    <property name="value">15</property>
    <property name="step_increment">5</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_disk">
    <property name="lower">50</property>
    <property name="upper">90</property>
    <property name="value">90</property>
    <property name="step_increment">5</property>
    <property name="page_increment">10</property>
  </object>

  <object class="GtkAboutDialog" id="about_dialog">
    <property name="program_name">GK Healter</property>
    <property name="version">0.1.2</property>
    <property name="copyright">© 2026 GK Developers</property>
    <property name="authors">Egehan KAHRAMAN, Mustafa GÖKPINAR</property>
    <property name="logo_icon_name">system-file-manager</property>
    <property name="transient_for">main_window</property>
  </object>

  <object class="GtkMessageDialog" id="clean_confirm_dialog">
    <property name="transient_for">main_window</property>
    <property name="message_type">warning</property>
    <property name="buttons">ok-cancel</property>
    <property name="text">Temizliği Onayla</property>
  </object>
</interface>
